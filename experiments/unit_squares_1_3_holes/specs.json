        "ClampingDistance": 1.0,
        "ToTrain": false,
        "LatentVectors": {
            "checkpoint": "latest",
            "num_iterations": 800,
            "lr": 0.005,
            "CodeRegularization" : true
        },
        "SamplesPerScene" : 10201
    },
    "DeepONet":{
        "NetworkArch": "DeepOnet2DGeometry",
        "NetworkSpecs" : {
            "num_branch_inputs": 1,
            "num_basis_functions": 128,
            "num_trunk_inputs": 34,
            "branch_dims": [128, 128, 128, 128],
            "trunk_dims": [128, 128, 128, 128]
        },
        "ToTrain": true,
        "ScenesPerBatch" : 64,
        "DataLoaderThreads" : 0,
        "NumEpochs" : 2000,
        "SnapshotFrequency" : 100,
        "AdditionalSnapshots" : [ 50 ],
        "SamplesPerScene": 200,
        "LearningRateSchedule" : [
            {
            "Type" : "Step",
            "Initial" : 0.0005,
            "Interval" : 500,
            "Factor" : 0.5
            }
        ]
    }
   
}